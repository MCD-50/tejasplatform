{
	"name": "platform",
	"version": "1.0.0",
	"description": "ticker platform",
	"main": "server.js",
	"scripts": {
		"start": "babel-node ./server.js --presets es2015,stage-2",
		"migrate-up": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate",
		"migrate-down": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate:undo",
		"migrate-down-all": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate:undo:all",
		"dev": "nodemon server.js --exec babel-node --presets es2015,stage-2",
		"server": "babel-node ./server.js --presets es2015,stage-2",
		"dev-monster": "npm run migrate-up && npm run dev",
		"server-monster": "npm run migrate-up && npm run server",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"async": "^2.6.1",
		"axios": "^0.19.2",
		"basic-auth": "^2.0.0",
		"bignumber.js": "^8.0.1",
		"body-parser": "^1.15.2",
		"cookie": "^0.3.1",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.3",
		"crypto-js": "^3.1.9-1",
		"dotenv": "^8.2.0",
		"express": "^4.14.0",
		"fs": "0.0.1-security",
		"joi": "^13.4.0",
		"jsonwebtoken": "^8.3.0",
		"kue": "^0.11.6",
		"kue-ui-express": "^1.0.2",
		"md5": "^2.2.1",
		"moment": "^2.21.0",
		"mongoose": "^5.5.11",
		"morgan": "^1.9.1",
		"node-cron": "^1.2.1",
		"querystring": "^0.2.0",
		"randomstring": "^1.1.5",
		"redis": "^2.8.0",
		"useragent": "^2.3.0",
		"uuid": "^3.2.1",
		"uuid-to-hex": "^1.0.3"
	},
	"devDependencies": {
		"babel-cli": "^6.11.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.9.0",
		"babel-preset-stage-2": "^6.11.0",
		"babel-register": "^6.11.6",
		"nodemon": "^1.11.0"
	}
}
